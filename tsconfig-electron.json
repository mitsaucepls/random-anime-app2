{
  "compilerOptions": {
    "module": "node16",
    "target": "ES2022",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "electron",
    "sourceMap": true,
    "types": ["node"]
  },
  "include": ["electron/**/*"],
}

